<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ZCMobileDemo.Lite.Views.Timesheet.Page5" StyleId="page5">
    <StackLayout Orientation="Vertical" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
        <Label x:Name="PageTitle" Text="{Binding Header1}" IsVisible="False"></Label>
        <Label Text="Page 5" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" TextColor="Red" FontSize="Large"></Label>
        <Button Text="Go to Page 6" Clicked="Button_Clicked" FontSize="Large"></Button>
    </StackLayout>
</ContentPage>